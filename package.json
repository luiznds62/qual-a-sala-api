{
    "name": "qual-a-sala-api",
    "version": "1.0.0",
    "description": "API para dados de asalas",
    "scripts": {
        "start": "npm run serve",
        "build": "babel . -d dist --presets es2015,stage-2 --ignore spec.js,node_modules",
        "serve": "node dist/server.js",
        "start_babel": "babel-node ./server.js --preset=babel-preset-es2015",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint ."
    },
    "eslintConfig": {
        "extends": "airbnb-base",
        "parserOptions": {
            "ecmaVersion": 8,
            "sourceType": "module"
        },
        "env": {
            "node": true,
            "es6": true
        },
        "rules": {
            "no-console": 0,
            "no-unused-vars": 1
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/luiznds62/qual-a-sala-api.git"
    },
    "keywords": [
        "Node.js",
        "Javascript",
        "Express"
    ],
    "author": "Luiz Henrique Naspolini",
    "dependencies": {
        "babel-runtime": "^6.26.0",
        "bcrypt": "^3.0.6",
        "body-parser": "^1.15.2",
        "cors": "^2.8.0",
        "express": "4.14.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.15.0",
        "mongoose": "^5.7.7",
        "morgan": "^1.8.2",
        "winston": "^2.3.1",
        "winston-daily-rotate-file": "^1.4.6"
    },
    "devDependencies": {
        "babel-cli": "6.16.0",
        "babel-core": "^6.17.0",
        "babel-eslint": "^7.0.0",
        "babel-plugin-inline-import": "^2.0.1",
        "babel-plugin-transform-async-to-module-method": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.16.0",
        "babel-preset-es2015": "^6.16.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-stage-0": "^6.16.0",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "^12.0.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^6.4.1",
        "eslint-plugin-standard": "^4.0.1",
        "nodemon": "^1.9.1"
    }
}